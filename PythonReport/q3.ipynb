   "source": [
    "# 시작값 i 1로 설정\n",
    "i = 1\n",
    "# 더한 값을 담을 sum 변수 설정\n",
    "sum = 0\n",
    "# i를 sum에 더한 후 i를 증가시켜 10이 될때까지 반복\n",
    "while i <= 10:\n",
    "    sum = sum + i\n",
    "    i = i + 1\n",
    "# 결과값 출력\n",
    "print(sum)"
